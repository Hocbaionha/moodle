<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/class_regist/db" VERSION="20160919" COMMENT="XMLDB file for Moodle local/class_regist"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="hbon_classes" COMMENT="classes">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="code" TYPE="char" LENGTH="50" NOTNULL="true" COMMENT="A1T2"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="false" COMMENT="A1 toi thu 2"/>
        <FIELD NAME="limited" TYPE="int" LENGTH="5" NOTNULL="false" COMMENT="40"/>
        <FIELD NAME="level" TYPE="char" LENGTH="50" NOTNULL="false" COMMENT="A1"/>
        <FIELD NAME="schedule" TYPE="char" LENGTH="255" NOTNULL="false" COMMENT="schedule"/>
        <FIELD NAME="linkzoom" TYPE="char" LENGTH="255" NOTNULL="false" COMMENT="linkzoom"/>
        <FIELD NAME="zoomid" TYPE="char" LENGTH="255" NOTNULL="false" COMMENT="zoomid"/>
        <FIELD NAME="zoompass" TYPE="char" LENGTH="255" NOTNULL="false" COMMENT="zoompass"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
     <TABLE NAME="hbon_schedule" COMMENT="hbon_schedule">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="code" TYPE="char" LENGTH="50" NOTNULL="true" COMMENT="A1T2"/>
        <FIELD NAME="day" TYPE="char" LENGTH="50" NOTNULL="false" COMMENT="17h30:19h"/>
        <FIELD NAME="time" TYPE="char" LENGTH="50" NOTNULL="false" COMMENT="17h30:19h"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="hbon_classes_register" COMMENT="hbon_classes_register">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="classid" TYPE="int" LENGTH="10" NOTNULL="true" COMMENT="A1T2"/>
        <FIELD NAME="created_at" TYPE="datetime" NOTNULL="false" DEFAULT="CURRENT_TIMESTAMP"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="false" COMMENT="ho ten hoc sinh"/>
        <FIELD NAME="class" TYPE="char" LENGTH="255" NOTNULL="false" COMMENT="lop"/>
        <FIELD NAME="school" TYPE="char" LENGTH="255" NOTNULL="false" COMMENT="truong"/>
        <FIELD NAME="province" TYPE="char" LENGTH="255" NOTNULL="false" COMMENT="tinh thanh pho"/>
        <FIELD NAME="phone" TYPE="char" LENGTH="255" NOTNULL="false" COMMENT="dien thoai"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>

    
  </TABLES>
</XMLDB>
